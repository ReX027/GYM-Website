//- Filling my index.pug code using base.pug
extends base.pug

block scripts
    script(src='/static/js/script.js')
    script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js' integrity='sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4' crossorigin='anonymous')
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65' crossorigin='anonymous')
    link(href='https://fonts.googleapis.com/css2?family=Baloo+Bhai+2&family=Bricolage+Grotesque:opsz@10..48&family=Comfortaa:wght@300&family=Noto+Sans+Multani&family=Noto+Serif+HK:wght@300;700&family=Teko:wght@300;500&family=Tilt+Prism&family=Ubuntu:ital,wght@1,300&family=Zilla+Slab:wght@300&display=swap' rel='stylesheet')
    link(rel='preconnect' href='https://fonts.googleapis.com')
    ink(rel='preconnect' href='https://fonts.gstatic.com' crossorigin='')

block style 
    style
        include ../static/css/style.css

block content 
    .container
      p Progress is born when you unleash your effort,not just at max reps.
    #carouselExampleCaptions.carousel.slide(data-bs-ride='carousel')
      .carousel-inner
        .carousel-item.active
          img.d-block.w-100(src='https://images.unsplash.com/photo-1548690312-e3b507d8c110?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MjB8fGd5bXxlbnwwfHwwfHx8MA%3D%3D&auto=format&fit=crop&w=500&q=60' alt='...')
          .carousel-caption.d-none.d-md-block
            h4 Free Trial
            p Try your first training for free Book here!!
        .carousel-item
          img(src='https://images.unsplash.com/photo-1590487988256-9ed24133863e?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MTd8fGd5bXxlbnwwfHwwfHx8MA%3D%3D&auto=format&fit=crop&w=500&q=60' width='405px' alt='...')
          .carousel-caption.d-none.d-md-block
            h4 Build your tomorrow
            p Equipped with top-tier gears for peak performance.
        .carousel-item
          img.d-block.w-100(src='https://images.unsplash.com/photo-1576913105965-1d0b6a19a482?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8OTV8fGd5bXxlbnwwfHwwfHx8MA%3D%3D&auto=format&fit=crop&w=500&q=60' alt='...')
          .carousel-caption.d-none.d-md-block
            h4 Join the Movement
            p Life has its ups and downs, We call them Squats
      button.carousel-control-prev(type='button' data-bs-target='#carouselExampleCaptions' data-bs-slide='prev')
        span.carousel-control-prev-icon(aria-hidden='true')
        span.visually-hidden Previous
      button.carousel-control-next(type='button' data-bs-target='#carouselExampleCaptions' data-bs-slide='next')
        span.carousel-control-next-icon(aria-hidden='true')
        span.visually-hidden Next

    
